{
  "name": "amino",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "dev": "concurrently 'next dev' 'quirrel'",
    "build": "prisma generate && next build",
    "migrate:new": "npx dotenv -e .env.local -- npx prisma migrate dev",
    "migrate:dev": "npx dotenv -e .env.local -- npx prisma migrate deploy",
    "migrate:stage": "npx dotenv -e .env.stage -- npx prisma migrate deploy",
    "migrate:prod": "npx dotenv -e .env.prod -- npx prisma migrate deploy",
    "migrate:reset-dev": "npx dotenv -e .env.local -- npx prisma db push --force-reset",
    "start": "next start",
    "lint": "next lint",
    "run:ts-node": "DOTENV_CONFIG_PATH=.env.local ts-node -r dotenv/config -r tsconfig-paths/register -P tsconfig.json"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.2",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@huggingface/inference": "^2.6.1",
    "@prisma/client": "^5.3.1",
    "@scaleleap/pg-format": "^1.0.0",
    "@tailwindcss/forms": "^0.5.6",
    "@tanstack/react-query": "^4.35.3",
    "@tremor/react": "^3.7.9",
    "@types/node": "20.3.1",
    "@types/react": "18.2.13",
    "@types/react-dom": "18.2.6",
    "@types/react-timeago": "^4.1.3",
    "apexcharts": "^3.42.0",
    "autoprefixer": "^10.4.16",
    "axios": "^1.5.0",
    "classnames": "^2.3.2",
    "cli-chart": "^0.3.1",
    "commander": "^11.0.0",
    "concurrently": "^8.2.1",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.50.0",
    "eslint-config-next": "^13.5.2",
    "formik": "^2.4.5",
    "gpt-tokenizer": "^2.1.1",
    "moment-timezone": "^0.5.43",
    "next": "^13.5.2",
    "next-auth": "^4.23.2",
    "nodemailer": "^6.9.5",
    "openai": "4.11.1",
    "pgvector": "^0.1.5",
    "postcss": "8.4.31",
    "quirrel": "^1.14.1",
    "react": "18.2.0",
    "react-apexcharts": "^1.4.1",
    "react-dom": "18.2.0",
    "react-number-format": "^5.3.1",
    "react-timeago": "^7.2.0",
    "sqlutils": "^1.5.3",
    "swr": "^2.2.2",
    "tailwindcss": "^3.3.3",
    "twilio": "^4.18.0",
    "typescript": "^5.2.2",
    "underscore": "^1.13.6",
    "vcards-js": "^2.10.0",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.6.3",
    "@types/nodemailer": "^6.4.10",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "@types/react-timeago": "^4.1.3",
    "@types/underscore": "^1.11.9",
    "@types/vcards-js": "^2.10.2",
    "prisma": "^5.3.1",
    "ts-node": "^10.9.1"
  }
}
